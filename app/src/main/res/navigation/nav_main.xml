<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_main"
    app:startDestination="@id/calendarFragment">

    <fragment
        android:id="@+id/calendarFragment"
        android:name="com.example.forestlearning.CalendarFragment"
        android:label="fragment_calendar"
        tools:layout="@layout/fragment_calendar" >
        <action
            android:id="@+id/action_calendarFragment_to_fruitshowFragment"
            app:destination="@id/fruitshowFragment" />
        <action
            android:id="@+id/action_calendarFragment_to_todaytreeFragment"
            app:destination="@id/todaytreeFragment" />
        <action
            android:id="@+id/action_calendarFragment_to_todolistFragment"
            app:destination="@id/todolistFragment" />
        <action
            android:id="@+id/action_calendarFragment_to_studytimeFragment"
            app:destination="@id/studytimeFragment" />
        <action
            android:id="@+id/action_calendarFragment_to_timetableFragment2"
            app:destination="@id/timetableFragment" />
    </fragment>
    <fragment
        android:id="@+id/fruitshowFragment"
        android:name="com.example.forestlearning.FruitshowFragment"
        android:label="fragment_fruitshow"
        tools:layout="@layout/fragment_fruitshow" />
    <fragment
        android:id="@+id/studytimeFragment"
        android:name="com.example.forestlearning.StudytimeFragment"
        android:label="fragment_studytime"
        tools:layout="@layout/fragment_studytime" >
        <action
            android:id="@+id/action_studytimeFragment_to_subjectadderFragment"
            app:destination="@id/subjectadderFragment" />
    </fragment>
    <fragment
        android:id="@+id/todolistFragment"
        android:name="com.example.forestlearning.TodolistFragment"
        android:label="fragment_todolist"
        tools:layout="@layout/fragment_todolist" />
    <fragment
        android:id="@+id/loginFragment"
        android:name="com.example.forestlearning.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_loginFragment_to_signInFragment"
            app:destination="@id/signInFragment" />
        <action
            android:id="@+id/action_loginFragment_to_calendarFragment"
            app:destination="@id/calendarFragment" />
    </fragment>
    <fragment
        android:id="@+id/signInFragment"
        android:name="com.example.forestlearning.SignInFragment"
        android:label="fragment_sign_in"
        tools:layout="@layout/fragment_sign_in" >
        <action
            android:id="@+id/action_signInFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
    </fragment>
    <fragment
        android:id="@+id/todaytreeFragment"
        android:name="com.example.forestlearning.TodaytreeFragment"
        android:label="fragment_todaytree"
        tools:layout="@layout/fragment_todaytree" />
    <fragment
        android:id="@+id/timetableFragment"
        android:name="com.example.forestlearning.TimetableFragment"
        android:label="fragment_timetable"
        tools:layout="@layout/fragment_timetable" >
        <action
            android:id="@+id/action_timetableFragment_to_courseFragment"
            app:destination="@id/courseFragment" />
    </fragment>
    <fragment
        android:id="@+id/courseFragment"
        android:name="com.example.forestlearning.CourseFragment"
        android:label="fragment_course"
        tools:layout="@layout/fragment_course" >
        <action
            android:id="@+id/action_courseFragment_to_timetableFragment"
            app:destination="@id/timetableFragment" />
    </fragment>
    <fragment
        android:id="@+id/subjectadderFragment"
        android:name="com.example.forestlearning.SubjectadderFragment"
        android:label="fragment_subjectadder"
        tools:layout="@layout/fragment_subjectadder" />
</navigation>